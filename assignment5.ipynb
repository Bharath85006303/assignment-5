    "#Write a Python class to implement pow(x, n)\n",
    "class power:\n",
    "    def __init__(self,x,n):\n",
    "        self.x=x\n",
    "        self.n=n\n",
    "    def ans(self):\n",
    "        return self.x**self.n\n",
    "x=int(input())\n",
    "n=int(input())\n",
    "t=power(x,n)\n",
    "t.ans()"
   ]
